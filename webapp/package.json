  },
  "repository": {
    "type": "git",
    "url": "https://gitenterprise.inside-box.net/Box/l10n/webapp"
  },
  "scripts": {
    "start-dev": "npm run start-server & npm run watch",
    "start-server": "mvn spring-boot:run -P '!springbootconfigprocessor' -Dspring.config.location=file:///$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER",
    "start-server-debug": "mvn spring-boot:run -Drun.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=5005\" -P '!springbootconfigprocessor' -Dspring.config.location=file:///$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER",
    "watch": "npm run watch-rb & grunt watch & npm run watch-js",
    "watch-resources": "nodemon --watch src/main/resources/ -e html,css --exec 'mvn resources:resources'",
    "watch-js": "webpack --progress --colors --watch --inline-source-map",
    "watch-rb": "mojito-rb-gen -w -s src/main/resources/properties/ -o target/classes/public/intl/ -t js",
    "build-rb": "mojito-rb-gen -s src/main/resources/properties/ -o target/classes/public/intl/ -t js",
    "build": "webpack --progress --minimize",
    "tests": "./node_modules/karma/bin/karma start --singleRun=true",
    "mojito-tm-import": "java -jar ../cli/target/mojito-cli-*.jar tm-import -r Mojito -s src/main/resources/xliff/",
    "mojito-tm-export": "java -jar ../cli/target/mojito-cli-*.jar tm-export -r Mojito -t src/main/resources/xliff/",
    "mojito-push": "java -jar ../cli/target/mojito-cli-*.jar push -r Mojito -s src/main/resources/properties/ -ft PROPERTIES_NOBASENAME",
    "mojito-pull": "cd src/main/resources/properties/; java -jar ../../../../../cli/target/mojito-cli-*.jar pull -r Mojito -lm \"fr:fr-FR,be:be-BY,ko:ko-KR,ru:ru-RU\" -ft PROPERTIES_NOBASENAME",
    "mojito-drop-export": "java -jar ../cli/target/mojito-cli-*.jar drop-export -r Mojito",
    "mojito-drop-import": "java -jar ../cli/target/mojito-cli-*.jar drop-import -r Mojito",
    "create-demo-data": "java -jar ../cli/target/mojito-cli-*.jar demo-create -n Demo"
  },
  "author": "Moji Webapp Team"
}
